<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.auxviewdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".InstallLoaction" android:label="               安装位置"
            android:theme="@style/Theme.AppCompat.Light"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InstallLoaction" />
        </activity>

        <activity android:name=".WifiConnection" android:label="               关联Wi-Fi"
            android:theme="@style/Theme.AppCompat.Light"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WifiConnection" />

        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>